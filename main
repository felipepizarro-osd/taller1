package clases;

import java.io.File;
import java.io.FileNotFoundException;
import java.util.*;

public class main {
//user registration
	//username *base de datos no repetir nombres de usuario
	//saldo * pedir a la 
	//
//user sign in 
//user sign out

	public static void main (String[] args)
	{
		try
		{
			File f = new File("E:/TALLER1/cajas.txt");
			Scanner Sc = new Scanner(f);
			int it = 0;
			while(Sc.hasNextLine()) 
			{
				String linea =Sc.nextLine();
				System.out.println(linea);
				String[] partes = linea.split(",");
				double precio = Double.valueOf(partes[1]);
				int stock = Integer.parseInt(partes[2]);
				switch(it)
				{
					case 0 :
						Cajas c1 = new Cajas(partes[0],precio,stock);
						it++;
						System.out.println(c1.toString());
						break;
					case 1 :
						Cajas c2 = new Cajas(partes[0],precio,stock);
						it++;
						System.out.println(c2.toString());
						break;
					case 2 :
						Cajas c3 = new Cajas(partes[0],precio,stock);
						it++;
						System.out.println(c3.toString());
						break;
					case 3 :
						Cajas c4 = new Cajas(partes[0],precio,stock);
						it++;
						System.out.println(c4.toString());
						break;
					case 4 :
						Cajas c5 = new Cajas(partes[0],precio,stock);
						it++;
						System.out.println(c5.toString());
						break;
				}
						
				
			}
			
			
		}catch(FileNotFoundException e)
		{
			e.printStackTrace();
		}
		//leerDatos("E:/TALLER1/cajas.txt");
		
		
	}


	
	
}
